<?xml version="1.0" encoding="utf-8"?>

     <androidx.constraintlayout.widget.ConstraintLayout  xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.example.boardgame.notification.testFragment">

         <ScrollView
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             app:layout_constraintBottom_toBottomOf="parent"
             app:layout_constraintEnd_toEndOf="parent"
             app:layout_constraintHorizontal_bias="1.0"
             app:layout_constraintStart_toStartOf="parent"
             app:layout_constraintTop_toTopOf="parent"
             app:layout_constraintVertical_bias="0.0">

             <androidx.constraintlayout.widget.ConstraintLayout
                 android:layout_width="match_parent"
                 android:layout_height="wrap_content">

                 <Button
                     android:id="@+id/inviteFriendButton"
                     android:layout_width="338dp"
                     android:layout_height="39dp"
                     android:layout_marginStart="32dp"
                     android:layout_marginTop="16dp"
                     android:layout_marginEnd="32dp"
                     android:text="inviteFriendButton"
                     app:layout_constraintEnd_toEndOf="parent"
                     app:layout_constraintHorizontal_bias="0.555"
                     app:layout_constraintStart_toStartOf="parent"
                     app:layout_constraintTop_toTopOf="parent" />

                 <Button
                     android:id="@+id/addFriendButton"
                     android:layout_width="0dp"
                     android:layout_height="wrap_content"
                     android:text="addFriendButton"
                     app:layout_constraintEnd_toEndOf="@+id/inviteFriendButton"
                     app:layout_constraintHorizontal_bias="0.0"
                     app:layout_constraintStart_toStartOf="@+id/inviteFriendButton"
                     app:layout_constraintTop_toBottomOf="@+id/inviteFriendButton" />

                 <Button
                     android:id="@+id/reportPlayerButton"
                     android:layout_width="0dp"
                     android:layout_height="wrap_content"
                     android:layout_marginTop="48dp"
                     android:text="reportPlayerButton"
                     app:layout_constraintEnd_toEndOf="@+id/addFriendButton"
                     app:layout_constraintHorizontal_bias="0.0"
                     app:layout_constraintStart_toStartOf="@+id/addFriendButton"
                     app:layout_constraintTop_toBottomOf="@+id/addFriendButton" />

                 <Button
                     android:id="@+id/reportShopButton"
                     android:layout_width="0dp"
                     android:layout_height="wrap_content"
                     android:layout_marginTop="48dp"
                     android:text="reportShopButton"
                     app:layout_constraintEnd_toEndOf="@+id/inviteFriendButton"
                     app:layout_constraintHorizontal_bias="0.0"
                     app:layout_constraintStart_toStartOf="@+id/inviteFriendButton"
                     app:layout_constraintTop_toBottomOf="@+id/reportPlayerButton" />

                 <Button
                     android:id="@+id/inviteGroupButton"
                     android:layout_width="0dp"
                     android:layout_height="wrap_content"
                     android:layout_marginTop="48dp"
                     android:text="inviteGroupButton"
                     app:layout_constraintEnd_toEndOf="@+id/inviteFriendButton"
                     app:layout_constraintHorizontal_bias="0.0"
                     app:layout_constraintStart_toStartOf="@+id/inviteFriendButton"
                     app:layout_constraintTop_toBottomOf="@+id/inviteFriendButton" />

                 <Button
                     android:id="@+id/groupCheckButton"
                     android:layout_width="0dp"
                     android:layout_height="wrap_content"
                     android:layout_marginTop="240dp"
                     android:text="groupCheckButton"
                     app:layout_constraintEnd_toEndOf="@+id/inviteFriendButton"
                     app:layout_constraintHorizontal_bias="0.0"
                     app:layout_constraintStart_toStartOf="@+id/inviteFriendButton"
                     app:layout_constraintTop_toBottomOf="@+id/inviteFriendButton" />

                 <Button
                     android:id="@+id/refuseGroupButton"
                     android:layout_width="0dp"
                     android:layout_height="wrap_content"
                     android:layout_marginTop="288dp"
                     android:text="refuseGroupButton"
                     app:layout_constraintEnd_toEndOf="@+id/inviteFriendButton"
                     app:layout_constraintHorizontal_bias="0.0"
                     app:layout_constraintStart_toStartOf="@+id/inviteFriendButton"
                     app:layout_constraintTop_toBottomOf="@+id/inviteFriendButton" />

                 <Button
                     android:id="@+id/reportGroupButton"
                     android:layout_width="0dp"
                     android:layout_height="wrap_content"
                     android:layout_marginTop="144dp"
                     android:text="reportGroupButton"
                     app:layout_constraintEnd_toEndOf="@+id/inviteFriendButton"
                     app:layout_constraintHorizontal_bias="0.0"
                     app:layout_constraintStart_toStartOf="@+id/inviteFriendButton"
                     app:layout_constraintTop_toBottomOf="@+id/inviteFriendButton" />

                 <Button
                     android:id="@+id/adButton"
                     android:layout_width="0dp"
                     android:layout_height="wrap_content"
                     android:layout_marginTop="336dp"
                     android:text="adButton"
                     app:layout_constraintEnd_toEndOf="@+id/inviteFriendButton"
                     app:layout_constraintHorizontal_bias="0.0"
                     app:layout_constraintStart_toStartOf="@+id/inviteFriendButton"
                     app:layout_constraintTop_toBottomOf="@+id/inviteFriendButton" />

                 <Button
                     android:id="@+id/refuseAdButton"
                     android:layout_width="0dp"
                     android:layout_height="wrap_content"
                     android:layout_marginTop="384dp"
                     android:text="refuseAdButton"
                     app:layout_constraintEnd_toEndOf="@+id/inviteFriendButton"
                     app:layout_constraintHorizontal_bias="0.0"
                     app:layout_constraintStart_toStartOf="@+id/inviteFriendButton"
                     app:layout_constraintTop_toBottomOf="@+id/inviteFriendButton" />

                 <Button
                     android:id="@+id/playerNosList"
                     android:layout_width="0dp"
                     android:layout_height="wrap_content"
                     android:layout_marginTop="475dp"
                     android:layout_marginBottom="180dp"
                     android:text="playerNosList"
                     app:layout_constraintBottom_toBottomOf="parent"
                     app:layout_constraintEnd_toEndOf="@+id/inviteFriendButton"
                     app:layout_constraintHorizontal_bias="0.0"
                     app:layout_constraintStart_toStartOf="@+id/inviteFriendButton"
                     app:layout_constraintTop_toBottomOf="@+id/inviteFriendButton"
                     app:layout_constraintVertical_bias="1.0" />

                 <Button
                     android:id="@+id/shopNosList"
                     android:layout_width="0dp"
                     android:layout_height="wrap_content"
                     android:layout_marginBottom="228dp"
                     android:text="shopNosList"
                     app:layout_constraintBottom_toBottomOf="parent"
                     app:layout_constraintEnd_toEndOf="@+id/inviteFriendButton"
                     app:layout_constraintHorizontal_bias="0.0"
                     app:layout_constraintStart_toStartOf="@+id/inviteFriendButton" />

                 <Button
                     android:id="@+id/sysbutton"
                     android:layout_width="0dp"
                     android:layout_height="49dp"
                     android:layout_marginBottom="128dp"
                     android:text="SystemNotification"
                     app:layout_constraintBottom_toBottomOf="parent"
                     app:layout_constraintEnd_toEndOf="@+id/inviteFriendButton"
                     app:layout_constraintHorizontal_bias="0.0"
                     app:layout_constraintStart_toStartOf="@+id/inviteFriendButton" />

             </androidx.constraintlayout.widget.ConstraintLayout>
         </ScrollView>
     </androidx.constraintlayout.widget.ConstraintLayout>
